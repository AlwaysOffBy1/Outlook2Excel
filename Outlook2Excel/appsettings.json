    "EmailMessageMapping": {
        "PurchaseOrderNumber": "REQ#:(.*)\\n",
        "Investigator": "PI:(.*)\\n",
        "ProjectCode": "Project:(.*)\\n",
        "Aup": "Protocol:(.*)\n",
        "Species": "Species:(.*)\n",
        "Strain": "Strain:(.*)\n",
        "Sex": "Sex:(.*)\n",
        "NumberOfAnimals": "Quantity:(.*)\n",
        "Supplier": "Vendor:(.*)\n",
        "DeliveryDate": "Date of Arrival(.*)\n",
        "Room": "Preferred Room #:(.*)\n",
        "Comments": "Comments:(\\r?\\n([\\s\\S]*?))(\\r?\\n)All inquiries"
    }
}